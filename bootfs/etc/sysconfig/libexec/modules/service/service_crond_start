service_crond_start(){
crond_prog=/usr/bin/crond
cat /dev/null > /var/log/crond.log
PID=`/bin/pidof crond`
[ "$PID" == "" ] || {
 for i in $PID; do
  /bin/kill -9 $i
 done
 }
${crond_prog} -L /var/log/crond.log >/dev/null 2>&1
[ $? -eq 0 ] &&\
 /usr/bin/logger "$0 - crond deamon Start Succeed" ||\
 /usr/bin/logger "$0 - crond deamon Start Failed"
}
