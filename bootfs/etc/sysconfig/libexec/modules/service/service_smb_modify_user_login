service_smb_modify_user_login(){
CONFIG_PATH=/etc/sysconfig/config
SMB_SHARES_CONF=${CONFIG_PATH}/smb/shares
Users=`/usr/bin/awk -F= /invalid/'{print $2}' ${SMB_SHARES_CONF}/${FolderName}.inc|/usr/bin/sed 's/\ //g'|/usr/bin/tr "," " "`

for user in $Users; do
 [ "$user" == "" ] && continue
 [ "$user" == "$UserName" ] && continue
 /bin/echo "$user,"
done
}